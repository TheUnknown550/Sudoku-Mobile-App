{
  "expo": {
    "name": "Sudoku Master",
    "slug": "Sudoku Master",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sudoku",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "googleMobileAdsAppId": "ca-app-pub-9992987709866316~3478776009"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "googleMobileAdsAppId": "ca-app-pub-9992987709866316~3478776009",
      "package": "com.anonymous.SudokuMaster"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-asset",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-9992987709866316~3478776009",
          "iosAppId": "ca-app-pub-9992987709866316~3478776009"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
